<%= link_to editor_path(editor) do %>
  <i class="fa fa-eye" title="Show"></i>
<% end %>
<%= link_to edit_editor_path(editor) do %>
  <i class="fa fa-edit" title="Edit"></i>
<% end %>
<%= link_to editor_path(editor), method: :delete, data: { confirm: "Are you sure? "} do %>
  <i class="fa fa-trash" title="Destroy">
<% end %>
